{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "trybe-fundamentals",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-alert": "off",
      "no-console": "off",
      "prefer-arrow-callback": "off",
      "func-names": "off",
      "no-unused-vars": "warn"
    }
}
